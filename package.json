{
  "name": "qrize",
  "version": "0.0.1",
  "description": "JavaScript client for qrize.me API",
  "engines": {
    "npm": ">=4"
  },
  "main": "dist/qrize.cjs.js",
  "module": "dist/qrize.esm.js",
  "browser": "dist/qrize.umd.js",
  "repository": {
    "url": "https://github.com/qrize/qrize",
    "type": "git"
  },
  "author": "Sergey Goliney <sergey@goliney.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "prettier": "prettier --write src/**/* test/**/*",
    "eslint": "eslint src/**/* test/**/*",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "pretest": "npm-run-all eslint-check eslint build",
    "test": "jest",
    "test:coverage": "npm run test -- --coverage",
    "test:report": "cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-preset-env": "1.6.1",
    "coveralls": "3.0.0",
    "eslint": "4.9.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.6.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-prettier": "2.3.1",
    "jest-cli": "21.2.1",
    "npm-run-all": "4.1.1",
    "prettier": "1.7.4",
    "prettier-eslint": "8.2.1",
    "rollup": "0.50.0",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-commonjs": "8.2.5",
    "rollup-plugin-json": "2.3.0",
    "rollup-plugin-node-resolve": "3.0.0"
  }
}
